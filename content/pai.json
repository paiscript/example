{
  "ajax": {
    "useSourceFiles": true,
    "framework1": "prototype",
    "framework": "jquery"
  },
  "content": {
    "header": {
      "type": "pagevar",
      "name": "title",
      "id": "header"
    },
    "page": {
      "type": "path",
      "default": "404",
      "id": "content"
    },
    "sidebar": {
      "type": "map",
      "default": "default",
      "map": {
        "test": ["test"],
        "default": ["index"]
      },
      "exclude": ["form"]
    },
    "intervaltest": {
      "type": "filter",
      "interval": {
        "pageChange": false,
        "frequency": 10,
        "decay": 1
      }
    },
    "breadcrumb": {
      "type": "filter"
    }
  },
  "plugins": {
    "locale": {
      "status": true,
      "available": ["en", "no"],
      "default": "en",
      "pageinfo": [
        "title",
        "description"
      ]
    },
    "menu": {
      "status": true,
      "currentClass": "active",
      "currentChildClass": "active",
      "ulclass": "nav navbar-nav",
      "list": {
        "main": {
          "currentClass": "active"
        }
      }
    },
    "piwik": {
      "status": true,
      "baseUrl": "//piwik.tsys.no/",
      "siteId": 6
    },
    "phperror": true,
    "analytics": {
      "status": false,
      "account": "UA-XXXXX-X",
      "anonymizeIp": false
    },
    "html5shiv": {
      "status": false
    }
  }
}